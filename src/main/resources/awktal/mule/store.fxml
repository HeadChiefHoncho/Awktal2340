<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="600.0" maxWidth="600.0" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="awktal.mule.StoreController">
   <center>
      <TabPane prefHeight="400.0" prefWidth="982.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
         <tabs>
            <Tab id="mule_pane" text="Mules">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="519.0" prefWidth="981.0">
                     <children>
                        <LineChart layoutX="7.0" layoutY="207.0" prefHeight="105.0" prefWidth="189.0">
                           <xAxis>
                              <CategoryAxis side="BOTTOM" />
                           </xAxis>
                           <yAxis>
                              <NumberAxis side="LEFT" />
                           </yAxis>
                        </LineChart>
                        <RadioButton layoutX="14.0" layoutY="32.0" mnemonicParsing="false" text="Buy" />
                        <RadioButton layoutX="15.0" layoutY="58.0" mnemonicParsing="false" text="Sell" />
                        <Slider blockIncrement="1.0" layoutX="37.0" layoutY="172.0" max="10.0" showTickLabels="true" showTickMarks="true" />
                        <Button id="done" layoutX="274.0" layoutY="314.0" mnemonicParsing="false" onAction="#attemptExit" text="Done" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
            <Tab id="FOOD_STORE" text="Food">
               <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <RadioButton layoutX="14.0" layoutY="39.0" mnemonicParsing="false" text="Buy" />
                        <RadioButton layoutX="14.0" layoutY="63.0" mnemonicParsing="false" text="Sell" />
                        <BarChart layoutY="214.0" prefHeight="124.0" prefWidth="185.0">
                           <xAxis>
                              <CategoryAxis side="BOTTOM" />
                           </xAxis>
                           <yAxis>
                              <NumberAxis side="LEFT" />
                           </yAxis>
                        </BarChart>
                        <Slider blockIncrement="30.0" layoutX="30.0" layoutY="152.0" max="700.0" showTickLabels="true" showTickMarks="true" />
                        <Button layoutX="267.0" layoutY="308.0" mnemonicParsing="false" text="Done" />
                     </children>
                  </AnchorPane>
               </content>
            </Tab>
         </tabs>
      </TabPane>
   </center>
   <top>
      <HBox alignment="TOP_CENTER" spacing="30.0" BorderPane.alignment="CENTER">
         <children>
            <HBox>
               <children>
                  <Label text="Current Player: ">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
                  <Label fx:id="playerLabel" text="Label">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets left="5.0" />
               </padding>
            </HBox>
            <HBox alignment="TOP_CENTER">
               <children>
                  <Label text="Score: ">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
                  <Label fx:id="scoreLabel" text="Label">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox alignment="TOP_RIGHT" layoutX="576.0" layoutY="10.0">
               <children>
                  <Label text="Time: ">
                     <font>
                        <Font size="17.0" />
                     </font>
                     <HBox.margin>
                        <Insets />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="timeLabel" text="Label">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </Label>
               </children>
               <padding>
                  <Insets right="5.0" />
               </padding>
            </HBox>
         </children>
      </HBox>
   </top>
</BorderPane>
